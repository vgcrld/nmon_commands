#!/usr/bin/env ruby

require 'nmon_commands'
require 'optimist'

opts = Optimist::options do
  opt :customer, "Customer", type: :string, required: true
  opt :uuid,     "UUID",     type: :string, required: true
end

files = NmonCommands.get_file_list opts[:customer], opts[:uuid]
data  = NmonCommands.get files
